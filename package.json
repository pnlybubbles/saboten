    "typescript": "5.3.3"
  },
  "scripts": {
    "clean": "rimraf dist .netlify/edge-functions-dist",
    "dev": "yarn clean && netlify dev",
    "build": "yarn clean && netlify build",
    "deploy": "dotenv -- sh -c 'yarn build && netlify deploy'",
    "deploy:prod": "dotenv -- netlify deploy --prod",
    "fmt": "prettier . --write && eslint --fix .",
    "db:push": "deno run -A --unstable npm:prisma db push",
    "db:generate": "deno run -A --unstable npm:prisma generate --data-proxy"
  },
  "alias": {
    "@/*": "./src/app/$1",
    "@shared/*": "./netlify/edge-functions/_shared/$1"
  }
}
