{
  "type": "module",
  "engines": {
    "npm": "PLEASE USE PNPM!",
    "node": "v22.2.0"
  },
  "packageManager": "pnpm@9.2.0",
  "dependencies": {
    "@hono/zod-validator": "^0.5.0",
    "clsx": "^2.1.1",
    "currency-codes": "^2.2.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.2",
    "hono": "^4.7.10",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "string-hash": "^1.1.3",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "zod": "3.25.28"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250524.0",
    "@tailwindcss/vite": "^4.1.7",
    "@tsconfig/strictest": "^2.0.5",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/string-hash": "^1.1.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "better-sqlite3": "^11.10.0",
    "chalk": "^5.4.1",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "http-proxy": "^1.18.1",
    "npm-run-all": "^4.1.5",
    "oxlint": "^0.16.11",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.16.1"
  },
  "scripts": {
    "dev": "run-p 'dev:*'",
    "dev:worker": "wrangler dev",
    "dev:app": "vite",
    "dev:proxy": "tsx script/proxy.ts",
    "db:apply": "wrangler d1 migrations apply saboten --local",
    "db:push": "drizzle-kit push:sqlite",
    "db:migrate": "drizzle-kit generate:sqlite",
    "db:studio": "drizzle-kit studio",
    "deploy": "run-s 'deploy:*'",
    "deploy:db": "wrangler d1 migrations apply saboten --remote",
    "deploy:worker": "wrangler deploy",
    "deploy:app": "vite build && wrangler pages deploy dist",
    "fmt": "run-p 'fmt:*'",
    "fmt:prettier": "prettier --write .",
    "fmt:oxlint": "oxlint --fix",
    "lint": "run-p 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:oxlint": "oxlint",
    "typecheck": "run-p 'typecheck:*'",
    "typecheck:node": "tsc -p tsconfig.node.json",
    "typecheck:app": "tsc -p src/app",
    "typecheck:worker": "tsc -p src/api",
    "typecheck:db": "tsc -p src/db",
    "typecheck:util": "tsc -p src/util"
  }
}
